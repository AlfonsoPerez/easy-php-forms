{
    "name":"hellofuture/customforms",
    "type":"cms",
    "description":"Custom forms for your php website",
    "license":"MIT",
    "authors":[
        {
            "name":"Alfonso Perez"
        }
    ],
    "require":{
        "php":">=5.3.0",
        "slim/slim":"2.*",
        "slim/extras": "dev-feature/no_static_views",
        "slim/views":"0.1.*",
        "illuminate/database":"*",
        "swiftmailer/swiftmailer":"*",
        "twig/twig":"1.*"
